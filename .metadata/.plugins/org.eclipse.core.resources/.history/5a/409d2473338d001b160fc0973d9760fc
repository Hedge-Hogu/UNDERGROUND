import java.util.stream.IntStream;

public class Ex14_0 {
	public static void main(String[] args) {
		IntStream intStream = IntStream.of(1, 2, 2, 3, 3, 4, 5, 5, 6);
		intStream.forEach(System.out::print);
		System.out.println();
		intStream = IntStream.of(1, 2, 2, 3, 3, 4, 5, 5, 6);
		intStream.distinct().forEach(System.out::print);
		System.out.println();
		intStream = IntStream.of(1, 2, 2, 3, 3, 4, 5, 5, 6).distinct();
		intStream.filter(i -> i%2!=0 && i -> i%3!=0;).forEach(System.out::print);
	}

}



